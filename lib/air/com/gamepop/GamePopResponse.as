package com.gamepop{	// relies on as3corelib:	// https://github.com/mikechambers/as3corelib	import com.adobe.serialization.json.JSON;		public class GamePopResponse	{		[readonly]		private var _data    : Object;				public static function responseWithJSON(json:String) : GamePopResponse		{			var data     : Object = JSON.decode(json);			var response : GamePopResponse = new GamePopResponse();			response._data   = data;						return response;		}				public function get ok():Boolean		{			return _data.ok;		}				public function get string():String		{			return JSON.encode(_data);		}				public function get data():Object		{			return _data;		}			}}