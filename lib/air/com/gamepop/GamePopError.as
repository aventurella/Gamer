package com.gamepop{	public class GamePopError	{		public static const ERROR_UNAUTHORIZED_CLIENT : int = 1001;		public static const ERROR_NOT_IMPLEMENTED     : int = 1002;		public static const ERROR_UNKNOWN_GAME        : int = 2001;		public static const ERROR_UNKNOWN_GAME_MODE   : int = 2002;		public static const ERROR_UNKNOWN_PLATFORM    : int = 2003;		public static const ERROR_UNKNOWN_ERROR       : int = 2004;		public static const ERROR_UNKNOWN_USER        : int = 2005;		public static const ERROR_UNKNOWN_MATCH       : int = 2006;		public static const ERROR_UNKNOWN_ALIAS       : int = 2007;		public static const ERROR_SCHEDULE_TIME       : int = 3001;		public static const ERROR_CREATE_MATCH        : int = 3002;		public static const ERROR_UPDATE_MATCH        : int = 3003;		public static const ERROR_MATCH_FULL          : int = 3004;		public static const ERROR_DUPLICATE_PLAYER    : int = 3005;		public static const ERROR_INVALID_ALIAS       : int = 3006;		public static const ERROR_UPDATE_ALIAS        : int = 3007;				[readonly]		private var _code    : int;		private var _message : String;				public function GamePopError(code:int, message:String)		{			this._code    = code;			this._message = message;		}				public function get code():int		{			return _code;		}				public function get message():String		{			return _message;		}	}}